{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for React, Redux and Webpack 2",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf public",
    "dev": "SET NODE_ENV=development && webpack-dev-server --hot --inline --progress --colors --content-base ./public/ --port 9000",
    "build": "rimraf public && SET NODE_ENV=production && webpack -p",
    "deploy": "npm run build && git subtree push --prefix public origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanjunior/react-boilerplate.git"
  },
  "author": "Ivan Junior",
  "email": "vanjunior89@gmail.com",
  "license": "MIT",
  "keywords": [
    "react",
    "babel",
    "es2015",
    "webpack",
    "sass",
    "es6"
  ],
  "bugs": {
    "url": "https://github.com/vanjunior/react-boilerplate/issues"
  },
  "homepage": "https://vanjunior.github.io/react-boilerplate/",
  "dependencies": {
    "axios": "^0.15.3",
    "express": "^4.14.1",
    "foundation-sites": "^6.3.0",
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
